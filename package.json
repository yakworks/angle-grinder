{
  "name": "angle-grinder",
  "license": "MIT",
  "version": "3.0.0",
  "scripts": {
    "build": "rimraf dist && yarn build:wip",
    "build:dev": "webpack --config config/webpack.config.js --mode=development --bail --profile --progress",
    "build:wip": "webpack --config config/webpack.config.js --mode=production --bail --profile --progress",
    "build:docs": "webpack --mode=production --bail --profile --progress --content-base ./docs",
    "build:app": "webpack --bail --profile --progress --mode=production",
    "clean": "rimraf dist && rimraf build",
    "test": "eslint src/ && karma start tests/karma.conf.js --single-run --no-auto-watch --no-sandbox $*",
    "test-jasmine": "karma start tests/karma-jasmine.conf.js --single-run --no-auto-watch --no-sandbox $*",
    "lint": "eslint src/",
    "lint-demo": "eslint examples/demo/",
    "lint-fix": "eslint src/ --fix",
    "lint-demo-fix": "eslint examples/demo/ --fix",
    "start-docs": "webpack-dev-server --mode=development --content-base ./docs ",
    "start-sandbox": "webpack-dev-server ---mode=development --content-base ./examples/sandbox",
    "start-demo": "webpack-dev-server --config examples/demo/webpack.config.js --mode=development --content-base ./examples/demo",
    "build-demo": "rimraf dist && webpack --config examples/demo/webpack.config.js --mode=production --content-base ./examples/demo",
    "start-rdash": "webpack-dev-server --mode=development --content-base ./examples/rdash ",
    "start-ag-demo": "webpack-dev-server --mode=development --content-base ./examples/ag-demo-ui",
    "build-ag-demo": "webpack --mode=production --profile --progress --content-base ./examples/ag-demo-ui --pathout ./examples/ag-demo-grails/src/main/webapp",
    "watch-ag-demo": "webpack --watch --mode=development --config examples/ag-demo-ui/webpack.config.js --progress --content-base ./examples/ag-demo-ui --pathout ./examples/ag-demo-grails/src/main/webapp",
    "parcel": "parcel examples/simple/index.html",
    "parcel:build": "parcel build examples/simple/index.html",
    "jest": "jest"
  },
